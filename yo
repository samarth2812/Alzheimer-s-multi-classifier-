prisma.indentAuditTrail.count({
        select: {
          _count: {
            select: {
              _all: true
            }
          }
        },
        where: {
          AND: [
            {
              role: "COMPLIANCE_MAKER"
            },
            {
              OR: [
                {
                  actionType: {
                    contains: "cmm",
                    mode: "insensitive"
                  }
                },
                {
                  performedByName: {
                    contains: "cmm",
                    mode: "insensitive"
                  }
                },
                {
                  performedByEmail: {
                    contains: "cmm",
                    mode: "insensitive"
                  }
                }
              ]
            }
          ]
        }
      })

