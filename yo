    const fetchUrl = `${process.env.REACT_APP_BACKEND_URL}/api/v1/communication?type=${typeParam}&userEmail=${user.email}`;
export const getCommunication = async (
  req: Request,
  res: Response,
  next: NextFunction
): Promise<void> => {
  try {
    console.log(4, req.params);
    const type = req.params.type as any;
    const { userEmail, threadId } = req.params;
    const communications = await fetchCommunications(type, userEmail, threadId);
    res.status(200).json(communications);
  } catch (error) {
    next(error);
  }
};
