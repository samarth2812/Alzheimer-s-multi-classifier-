                department: item.department,
                dueDate: item.dueDate,
                ...(moduleId === "indents"
                  ? { category: item.category }
                  : moduleId === "incidents"
                  ? { type: item.type }
                  : moduleId === "ise-observation"
                  ? { theme: item.observationTheme }
                  : {}),


 obligationId: item.id,
                obligationTitle: item.title,

              moduleId === "indents"
                ? "INDENT"
                : moduleId === "incidents"
                ? "INCIDENT"
                : moduleId === "ise-observation"
                ? "OBSERVATION"
                : "",
